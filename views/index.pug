    .index-search-wrapper(style=setFrontDisplayStyle(page, 'index/search'))
      .index-search-input-wrapper
      .index-search-category-button-wrapper
        .index-search-category-each-button#index-search-category-each-button-all(class=(
          index_search_category != 'mainnet' && index_search_category != 'testnet' ? 'index-search-category-each-button-selected' : ''
        ))= __('index-search-category-each-button-all')
        .index-search-category-each-button#index-search-category-each-button-testnet(class=(
          index_search_category == 'testnet' ? 'index-search-category-each-button-selected' : ''
        ))= __('index-search-category-each-button-testnet')
        .index-search-category-each-button#index-search-category-each-button-mainnet(class=(
          index_search_category == 'mainnet' ? 'index-search-category-each-button-selected' : ''
        ))= __('index-search-category-each-button-mainnet')
      .index-search-category-selected-title#index-search-category-selected-title-all(style=(
        index_search_category == 'mainnet' || index_search_category == 'testnet' ? 'display: none' : ''
      ))= __('index-search-category-selected-title-all')
