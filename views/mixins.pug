mixin index-login-project-wrapper(project, will_install)
  a.index-login-project-link(href=project.urls.web target='_blank')
    svg.index-login-project-link-icon(viewBox='0 0 32 32' xmlns='http://www.w3.org/2000/svg')
      path(d='M24.9095 23.4832L22.7918 10.6705L24.5345 8.92782L22.9629 7.3562L21.2202 9.0989L8.87207 7.30249L6.61611 9.55845L20.6852 11.548L20.1803 11.0193L22.57 25.8227L24.9095 23.4832Z')
      rect(x='21.3916' y='12.0713' width='17.2251' height='2.2226' transform='rotate(135 21.3916 12.0713)')
  .index-login-project-header
    img.index-login-project-image(src=project.image[project.image.length - 1].url alt=`${__('index-login-project-image-alt')} ${project.name}`)
    .index-login-project-header-content-wrapper
      .index-login-project-title= project.name
      .index-login-project-description= project.chain_registry_identifier
      .index-login-project-incentivized(class=project.is_incentivized ? '' : 'display-none')= __('index-login-project-incentivized')
  .index-login-project-stake-with-us= project.description
  .index-login-project-installation-information-wrapper(class=will_install ? '' : 'display-none')
    input.display-none#index-login-project-identifier(value=project.chain_registry_identifier)
    .index-login-project-requirements-wrapper
      .index-login-project-requirements-title= __('index-login-project-requirements-title')
      .index-login-project-requirements-inner-wrapper
        .index-login-project-each-requirement-wrapper
          .index-login-project-each-requirement-title= __('index-login-project-each-requirement-title-cpu')
          .index-login-project-each-requirement-text= project.system_requirements.cpu
        .index-login-project-each-requirement-wrapper
          .index-login-project-each-requirement-title= __('index-login-project-each-requirement-title-ram')
          .index-login-project-each-requirement-text= project.system_requirements.ram
        .index-login-project-each-requirement-wrapper
          .index-login-project-each-requirement-title= __('index-login-project-each-requirement-title-storage')
          .index-login-project-each-requirement-text= project.system_requirements.storage
        .index-login-project-each-requirement-wrapper
          .index-login-project-each-requirement-title= __('index-login-project-each-requirement-title-os')
          .index-login-project-each-requirement-text= project.system_requirements.os
    .index-login-project-rent-server-title= __('index-login-project-rent-server-title')
    .index-login-project-rent-server-wrapper
      each rent_server, i in Object.keys(rent_servers)
        a.index-login-project-each-rent-server(href=rent_servers[rent_server] target='_blank')= rent_server
        if i != Object.keys(rent_servers).length - 1
          .index-login-project-each-rent-server-divider
mixin short-input-wrapper(input, id)
  .node-general-each-short-input-wrapper
    .node-general-each-short-input-title(class=(input ? 'node-general-each-short-input-title-required' : ''))= input.title
    input.node-general-each-short-input(id=input.id type=input.type pattern=(input.pattern ? input.pattern : '') placeholder=input.placeholder)
    .node-general-each-short-input-error.display-none= __('node-general-each-short-input-error')
mixin long-input-wrapper(input, id)
  .node-general-each-long-input-wrapper
    .node-general-each-long-input-title(class=(input ? 'node-general-each-long-input-title-required' : ''))= input.title
    input.node-general-each-long-input(id=id type=input.type placeholder=input.placeholder)
    .node-general-each-long-input-error.display-none
